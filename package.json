{
  "name": "vite-plugin-gfont",
  "version": "0.2.0",
  "description": "Automatically inject google font tags into your page with Vite",
  "main": "dist/vite-plugin-gfont.umd.js",
  "module": "dist/vite-plugin-gfont.esm.js",
  "typings": "dist/types/main.d.ts",
  "keywords": [
    "plugin",
    "google",
    "font",
    "vite"
  ],
  "scripts": {
    "watch": "rollup -c rollup.config.ts -w",
    "build": "rimraf ./dist && tsc --module commonjs && rollup -c rollup.config.ts && rimraf ./dist/lib"
  },
  "author": "BackRunner",
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^21.1.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@types/node": "^16.18.61",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "eslint": "^8.53.0",
    "eslint-config-alloy": "^4.4.0",
    "eslint-config-prettier": "^8.10.0",
    "prettier": "^2.8.8",
    "rimraf": "^3.0.2",
    "rollup": "^2.79.1",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "rollup-plugin-typescript2": "^0.31.2",
    "tslib": "^2.6.2",
    "typescript": "^4.9.5",
    "vite": "^2.9.16"
  }
}
